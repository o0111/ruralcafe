<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="RuralCafe.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    </configSections>
  <startup/>
  <userSettings>
    <RuralCafe.Properties.Settings>
      <setting name="LOCAL_PROXY_IP_ADDRESS" serializeAs="String">
        <value>127.0.0.1</value>
      </setting>
      <setting name="LOCAL_PROXY_LISTEN_PORT" serializeAs="String">
        <value>8080</value>
      </setting>
      <setting name="REMOTE_PROXY_IP_ADDRESS" serializeAs="String">
        <value>127.0.0.1</value>
      </setting>
      <setting name="REMOTE_PROXY_LISTEN_PORT" serializeAs="String">
        <value>8081</value>
      </setting>
      <setting name="EXTERNAL_PROXY_IP_ADDRESS" serializeAs="String">
        <value />
      </setting>
      <setting name="EXTERNAL_PROXY_LISTEN_PORT" serializeAs="String">
        <value>0</value>
      </setting>
      <setting name="EXTERNAL_PROXY_LOGIN" serializeAs="String">
        <value />
      </setting>
      <setting name="EXTERNAL_PROXY_PASS" serializeAs="String">
        <value />
      </setting>
      <setting name="DEFAULT_SEARCH_PAGE" serializeAs="String">
        <value>trotro.html</value>
      </setting>
      <setting name="DEFAULT_QUOTA" serializeAs="String">
        <value>2000000</value>
      </setting>
      <setting name="DEFAULT_DEPTH" serializeAs="String">
        <value>1</value>
      </setting>
      <setting name="MAXIMUM_DOWNLOAD_SPEED" serializeAs="String">
        <value>5000000</value>
      </setting>
      <setting name="INDEX_PATH" serializeAs="String">
        <value>index-computer_studies-test</value>
      </setting>
      <setting name="LOCAL_CACHE_PATH" serializeAs="String">
        <value>files-computer_studies-test</value>
      </setting>
      <setting name="REMOTE_CACHE_PATH" serializeAs="String">
        <value>Cache</value>
      </setting>
      <setting name="WIKI_DUMP_DIR" serializeAs="String">
        <value />
      </setting>
      <setting name="WIKI_DUMP_FILE" serializeAs="String">
        <value />
      </setting>
      <setting name="LOCAL_MAXIMUM_ACTIVE_REQUESTS" serializeAs="String">
        <value>50</value>
      </setting>
      <setting name="LOGLEVEL" serializeAs="String">
        <value>DEBUG</value>
      </setting>
      <setting name="DETECT_NETWORK_AUTO" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="DNS_CACHE_TTL" serializeAs="String">
        <value>604800</value>
      </setting>
      <setting name="LOCAL_MAX_INFLIGHT_REQUESTS" serializeAs="String">
        <value>5</value>
      </setting>
      <setting name="REMOTE_MAX_INFLIGHT_REQUESTS" serializeAs="String">
        <value>50</value>
      </setting>
    </RuralCafe.Properties.Settings>
  </userSettings>
  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ColoredConsoleAppender" >
      <mapping>
        <level value="WARN" />
        <backColor value="Yellow" />
      </mapping>
      <mapping>
        <level value="ERROR" />
        <foreColor value="White" />
        <backColor value="Red" />
      </mapping>
      <mapping>
        <level value="FATAL" />
        <foreColor value="White" />
        <backColor value="Red, HighIntensity" />
      </mapping>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger - %message%newline" />
      </layout>
    </appender>
    <appender name="RollingFileGeneral" type="log4net.Appender.RollingFileAppender">
      <file value="Logs/RuralCafe.log" />
      <staticLogFileName value="false" />
      <appendToFile value="true" />
      <maximumFileSize value="100KB" />
      <maxSizeRollBackups value="100" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger - %message%newline" />
      </layout>
    </appender>
    <appender name="RollingFileLocal" type="log4net.Appender.RollingFileAppender">
      <file value="LocalProxy/Logs/LocalProxy.log" />
      <staticLogFileName value="false" />
      <appendToFile value="true" />
      <maximumFileSize value="100KB" />
      <maxSizeRollBackups value="100" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger - %message%newline" />
      </layout>
    </appender>
    <appender name="RollingFileRemote" type="log4net.Appender.RollingFileAppender">
      <file value="RemoteProxy/Logs/RemoteProxy.log" />
      <staticLogFileName value="false" />
      <appendToFile value="true" />
      <maximumFileSize value="100KB" />
      <maxSizeRollBackups value="100" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="ConsoleAppender" />
      <appender-ref ref="RollingFileGeneral" />
    </root>
    <logger name="RuralCafe.RCLocalProxy">
      <appender-ref ref="RollingFileLocal" />
    </logger>
    <logger name="RuralCafe.RCRemoteProxy">
      <appender-ref ref="RollingFileRemote" />
    </logger>
  </log4net>
</configuration>
