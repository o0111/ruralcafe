*{
	padding:0;
	margin:0;
}


body{
	font-family: Verdana ,Arial, sans-serif;
	font-size:12px;
	margin: 0;
	height: 100%;
	max-height: 100%; 
	color:#222;
}

a:link{
	color:#2bb600;
}
a:visited{
	color:#1e7e00;
}

body a, .imp{	
	font-size:16px;
	font-weight:bold;
}

h2{
	margin-bottom:5px;
	font-size:20px;
}

.result_page{
	padding:10px;
	margin:5px 10px;
}
.url{
	color:#555;
	font-size:10px;
	line-height:10px;
	margin-bottom:1px;
	font-style:italic;
}
.cat_btn{
}

.cached_icon{
	margin-left:4px;
}

#count,#nav, #live_count, #live_nav{
	padding-left:10px;
}
#local{
	width:40%;
}

#live{
	float:right;
	width:40%;
	margin:0 5%;
}
